    <script>
        document.addEventListener('DOMContentLoaded', () => {
            // Section toggle functionality
            const headers = document.querySelectorAll('.section-header[data-section]');
            
            headers.forEach(header => {
                header.addEventListener('click', (e) => {
                    e.preventDefault();
                    const section = header.getAttribute('data-section');
                    const content = document.getElementById(`${section}-content`);
                    const card = header.closest('.section-card');
                    
                    if (!content || !card) {
                        console.error(`Content element for section ${section} not found`);
                        return;
                    }
                    
                    const isActive = content.classList.contains('active');
                    
                    // Close all other sections
                    document.querySelectorAll('.section-content.active').forEach(activeContent => {
                        activeContent.classList.remove('active');
                        activeContent.closest('.section-card').classList.remove('active');
                    });
                    
                    // Toggle current section
                    if (!isActive) {
                        content.classList.add('active');
                        card.classList.add('active');
                        
                        setTimeout(() => {
                            header.scrollIntoView({
                                behavior: 'smooth',
                                block: 'start',
                                inline: 'nearest'
                            });
                        }, 100);
                    }
                });
            });
            
            // Add staggered animation to cards
            const cards = document.querySelectorAll('.section-card');
            cards.forEach((card, index) => {
                card.style.opacity = '0';
                card.style.transform = 'translateY(20px)';
                
                setTimeout(() => {
                    card.style.transition = 'all 0.6s ease-out';
                    card.style.opacity = '1';
                    card.style.transform = 'translateY(0)';
                }, 100 + (index * 150));
            });
            
            // Add click tracking for analytics (optional)
            const newsLinks = document.querySelectorAll('.news-list a');
            newsLinks.forEach(link => {
                link.addEventListener('click', () => {
                    const title = link.querySelector('span').textContent;
                    const date = link.querySelector('.news-date').textContent;
                    console.log(`Clicked news article: ${title} - ${date}`);
                });
            });
        });
    </script>
</body>
